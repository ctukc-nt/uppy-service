<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <connectionStrings>
    <add name="mongoServer" connectionString="mongodb://192.168.1.222"/>
    <add name="dbName" connectionString="uppy"/>
  </connectionStrings>
  <system.serviceModel>
    <services>
      <service name="UPPY.Services.DataManagerService.UppyDataManagerService">
        <endpoint name="UppyDataService" binding="netTcpBinding" address="net.tcp://localhost:5450" contract="UPPY.Services.Core.IUppyDataService" bindingConfiguration="customTcpBinding" />
      </service>
    </services>
    <bindings>
      <netTcpBinding>
        <binding name="customTcpBinding" transferMode="Streamed" maxReceivedMessageSize="20480000" >
          <security mode="None"/>
          
        </binding>
      </netTcpBinding>
    </bindings>
  </system.serviceModel>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
</configuration>
